 <%= content_for :js do %>
 
 <script>
 $(function(){
    $(".issues-nav").load("<%= issues_path %>", function(html){
      ActivateIssuesIndex();
      $("#issue_<%= @issue.id%>").addClass("")
      // $("table.unresolved tr:first td a:first").click();
    });

    function pollIssuesIndex(){
      setTimeout(function(){
        $(".issues-nav").load("<%= issues_path %>", function(html){
          ActivateIssuesIndex();
          pollIssuesIndex();      
        })
      }, 5000)
    }
    pollIssuesIndex();

 });
 </script>
 <% end %>